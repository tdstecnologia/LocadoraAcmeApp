@using Microsoft.AspNetCore.Identity
@inject SignInManager<Usuario> SignInManager
@inject UserManager<Usuario> UserManager


@if (SignInManager.IsSignedIn(User))
{
    <ul class="navbar-nav">
        <li class="nav-item">
            Olá, @UserManager.GetUserName(User) - <a class="btn btn-link navbar-link" asp-controller="Usuarios" asp-action="Logout">Logout</a>
        </li>
    </ul>

}
